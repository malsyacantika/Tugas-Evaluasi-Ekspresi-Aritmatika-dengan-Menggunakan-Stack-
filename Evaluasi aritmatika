#include <iostream>
#include <stack>
#include <string>
#include <vector>
#include <cctype>

using namespace std;
int evaluatePostfix(const vector<string>& tokens){
  stack<int> operands;
  for (const string& token : tokens){
    if (isNum(token)){
      operands.push(stoi(token));
    }else{
      int operand2 = operands.top(); operands.pop();
      int operand1 = operands.top(); operands.top();
      if(token == "+") operands.push(operand1 + operand2);
      else if(token == "-") operands.push(operand1 - operand2);
      else if(token == "*") operands.push(operand1 * operand2);
      else if(token == "/") operands.push(operand1 / operand2);
      else if(token == '%') opernads.push(operand1 % operand2);
}
} 
return operand.top();


