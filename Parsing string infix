#include <iostream>
#include <vector> 
#include <string>
#include <cctype>
#include <stack>

using namespace std;

int prec (char op) {
  if (op == '+' || op == '-') return 1;
  if (op == '*' || op == '/'|| op == '%') return 2;
  return 0;
}

bool isOp (char ch){
return (ch == '+' || ch == '-' || ch == '*' || ch == '/'|| ch == '%');
}

bool isNum (const string& tok){
if (isdigit(tok[0])){
  return true ;
} else if(tok.size() > 1 && tok[0] == '-' && isdigit (tok[1])){
  return true;
}
  return false;
}

vector<string> parseInfix(const string& exp){
  vector<string> tokens;
  string num;
  for(size_t i=0;i<exp.size();++i){
  char ch = exp[i];
  if(isspace(ch)){
    continue;
  }
  if(isdigit(ch)){
    num+=ch;
  }else{
      if(!num.empty()){
          tokens.push_back(num);
          num.clear();
      }
      if(ch=='-'&&(i == 0 || exp[i-1] == '(' || exp [i-1] == '*' || exp[i-1]=='/' || exp[i-1] == '+' || exp [i-1] == '%' || exp[i-1] == '-')) {
        if (i+1<exp.size() && isdigit(exp[i+1])){
            num += ch;
        }else{
          tokens.push_back("-1");
          tokens.push_back("*");
        }
      }else if( ch == '-' && (tokens.empty() || tokens.back() == "(" || tokens.back() == "*" || tokens.back() == "+" || tokens.back == "-" || tokens.back() == "%")){
          tokens.push_back("-1");
          tokens.push_back("*");
        }else{
          tokens.push_back(string(1,ch));
        }
    }
}
if(!num.empty()){  
    tokens.push_back(num);
  }
  return tokens;
}
int main(){
  string input;
  getline(cin,input);

  vector<string> tokens = parseInfix(input);
  print_tokens(tokens);

return 0;
}
